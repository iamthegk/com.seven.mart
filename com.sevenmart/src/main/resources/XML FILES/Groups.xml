<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="group test" parallel="tests" thread-count="5">
	<listeners>
		<listener class-name="com.sevenmart.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="group tests">
		<groups>
			<run>
				<include name="smoke"></include>
			</run>
		</groups>
		<classes>
			<class name="com.sevenmart.tests.LoginTest"></class>
			<class name="com.sevenmart.tests.ManageDeliveryBoyTest"></class>
		</classes>

	</test>
</suite>